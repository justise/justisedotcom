{
    "modules": [
        {
            "dir": "<rootDir>/src/modules"
        }
    ],
    "application": {
        "name": "justise/homepage",
        "root": "justise/homepage"
    },
    "assetDirs": [
        {
            "alias": "assetsDir",
            "dir": "<rootDir>/src/assets",
            "path": "/public/assets"
        }
    ],
    "bundleConfig": {
        "exclude": ["lwc"],
        "external": {
            "dsc": "//cdn.foo.bar"
        }
    },
    "routes": [
        {
            "path": "/homepage",
            "source": "<contentDir>/homepage.md"
        }
    ]
}